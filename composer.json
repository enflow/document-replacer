{
    "name": "enflow/document-replacer",
    "description": "Modify docx templates and convert them to PDF",
    "keywords": [
        "enflow",
        "document",
        "docx"
    ],
    "homepage": "https://github.com/enflow-nl/document-replacer",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Bardelmeijer",
            "email": "michel@enflow.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "phpoffice/phpword": "^0.18.2",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Enflow\\DocumentReplacer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enflow\\DocumentReplacer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "bash tests.sh",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
